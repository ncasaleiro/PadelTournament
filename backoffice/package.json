    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "pkg": "^5.8.1",
    "supertest": "^6.3.3"
  },
  "pkg": {
    "assets": [
      "public/**/*"
