{
  "name": "padel-tournament-backoffice",
  "version": "1.0.0",
  "description": "Backoffice para gest√£o de torneios de Padel",
  "main": "src/server.js",
  "bin": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "init-db": "node src/database/init.js",
    "create-admin": "node src/database/create-admin.js",
    "validate-users": "node src/database/validate-users.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:win": "pkg . --targets node18-win-x64 --output dist/padel-backoffice.exe",
    "build:rpi64": "pkg . --targets node18-linux-arm64 --output dist/padel-backoffice-linux-arm64"
